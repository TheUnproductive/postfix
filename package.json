{
  "name": "pf",
  "displayName": "PostFix",
  "description": "LUH PostFix Language Support for VSCode",
  "version": "0.1.1",
  "publisher": "JustusSchneider",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "postfix",
      "aliases": ["Postfix", "postfix"],
      "extensions": [".pf"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "postfix",
      "scopeName": "source.pf",
      "path": "./syntaxes/postfix.tmLanguage.json"
    }]
  },
  "repository":{
    "type":"git","url":"https://github.com/TheUnproductive/postfix.git"
  }
}
